{
  "version": 2,
  "name": "pranitz-ai",
  "buildCommand": "cd client && npm install && npm run build",
  "outputDirectory": "client/build",
  "builds": [
    {
      "src": "api/chat.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/chat",
      "dest": "api/chat.js"
    },
    {
      "src": "/(.*)",
      "dest": "client/build/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
